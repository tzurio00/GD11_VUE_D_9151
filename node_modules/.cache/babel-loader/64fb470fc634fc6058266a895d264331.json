MAAA,CAAA,MAAA,GAAA,KAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,IAAA;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,WAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,KAAA;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,KAAA;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,KAAA;AACA,OAfA;AAgBA,KAnDA;AAoDA,IAAA,WApDA,uBAoDA,IApDA,EAoDA;AACA,WAAA,SAAA,GAAA,MAAA;AACA,WAAA,MAAA,GAAA,IAAA;AACA,WAAA,IAAA,CAAA,IAAA,GAAA,IAAA,CAAA,IAAA;AACA,WAAA,IAAA,CAAA,IAAA,GAAA,IAAA,CAAA,IAAA;AACA,WAAA,IAAA,CAAA,KAAA,GAAA,IAAA,CAAA,KAAA,EACA,KAAA,SAAA,GAAA,IAAA,CAAA,EADA;AAEA,KA3DA;AA4DA,IAAA,UA5DA,sBA4DA,QA5DA,EA4DA;AAAA;;AAAA;AACA,UAAA,GAAA,GAAA,KAAA,OAAA,GAAA,YAAA,GAAA,QAAA,CADA,CACA;;AACA,WAAA,KAAA,CAAA,MAAA,CAAA,GAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,IAAA;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,QAAA,CAAA,IAAA,CAAA,OAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,OAAA;AACA,QAAA,MAAA,CAAA,YAAA,GAAA,KAAA;;AACA,QAAA,MAAA,CAAA,OAAA;AACA,OANA,EAMA,KANA,CAMA,UAAA,KAAA,EAAA;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,KAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,IAAA;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,WAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,KAAA;AACA,OAXA;AAYA,KA1EA;AA2EA,IAAA,OA3EA,qBA2EA;AACA,UAAA,KAAA,SAAA,KAAA,KAAA,EAAA;AACA,aAAA,QAAA;AACA,OAFA,MAEA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,MAAA;AACA,aAAA,UAAA;AACA;AACA,KAlFA;AAmFA,IAAA,SAnFA,uBAmFA;AACA,WAAA,IAAA,GAAA;AACA,QAAA,IAAA,EAAA,EADA;AAEA,QAAA,IAAA,EAAA,EAFA;AAGA,QAAA,KAAA,EAAA;AAHA,OAAA;AAKA;AAzFA,GAhDA;AA2IA,EAAA,OA3IA,qBA2IA;AACA,SAAA,OAAA;AACA;AA7IA,CAAA","sourcesContent":["<template> \r\n    <v-container> \r\n        <v-card>\r\n            <v-container grid-list-md mb-0>\r\n                <h2 class=\"text-md-center\">Data Service</h2> \r\n                <v-layout row wrap style=\"margin:10px\"> \r\n                    <v-flex xs6> \r\n                        <v-btn\r\n                        depressed \r\n                        dark \r\n                        rounded \r\n                        style=\"text-transform: none !important;\" \r\n                        color = \"green accent-3\" \r\n                        @click=\"dialog = true\"\r\n                        >\r\n                        <v-icon size=\"18\" class=\"mr-2\">mdi-pencil-plus</v-icon>\r\n                            Tambah Service\r\n                        </v-btn>\r\n                    </v-flex>\r\n                    <v-flex xs6 class=\"text-right\"> \r\n                        <v-text-field \r\n                            v-model=\"keyword\"\r\n                            append-icon=\"mdi-search\"\r\n                            label=\"Search\" \r\n                            hide-details \r\n                        ></v-text-field>\r\n                    </v-flex> \r\n                </v-layout> \r\n            \r\n                <v-data-table \r\n                    :headers=\"headers\" \r\n                    :items=\"services\" \r\n                    :search=\"keyword\" \r\n                    :loading=\"load\" \r\n                > \r\n                    <template v-slot:body=\"{ items }\"> \r\n                        <tbody> \r\n                            <tr v-for=\"(item,index) in items\" :key=\"item.id\"> \r\n                                <td>{{ index + 1 }}</td> \r\n                                <td>{{ item.name }}</td>  \r\n                                <td>{{ item.type }}</td> \r\n                                <td>{{ item.price }}</td> \r\n                                <td>{{ item.created_at }}</td> \r\n                                <td class=\"text-center\"> \r\n                                    <v-btn \r\n                                        icon \r\n                                        color=\"indigo\" \r\n                                        light \r\n                                        @click=\"editHandler(item)\" \r\n                                    > \r\n                                        <v-icon>mdi-pencil</v-icon>\r\n                                    </v-btn>\r\n                                    <v-btn\r\n                                        icon\r\n                                        color=\"error\"\r\n                                        light \r\n                                        @click=\"deleteData(item.id)\"\r\n                                    > \r\n                                        <v-icon>mdi-delete</v-icon>\r\n                                    </v-btn> \r\n                                </td> \r\n                            </tr> \r\n                        </tbody> \r\n                    </template> \r\n                </v-data-table> \r\n            </v-container> \r\n        </v-card> \r\n        <v-dialog v-model=\"dialog\" persistent max-width=\"600px\"> \r\n            <v-card> \r\n                <v-card-title> \r\n                    <span class=\"headline\">Input Service</span> \r\n                </v-card-title> \r\n                <v-card-text> \r\n                    <v-container> \r\n                        <v-row> \r\n                            <v-col cols=\"12\"> \r\n                                <v-text-field label=\"Name*\" v-model=\"form.name\" required></v-text-field> \r\n                            </v-col> \r\n                            <v-col cols=\"12\"> \r\n                                <v-select :items =\"jenis\"\r\n                                label=\"Type\"\r\n                                v-model=\"form.type\">\r\n                                </v-select>\r\n                            </v-col>\r\n                            <v-col cols=\"12\"> \r\n                                <v-text-field type=\"number\" label=\"Price\" v-model=\"form.price\"  required></v-text-field> \r\n                            </v-col> \r\n                        </v-row> \r\n                    </v-container>\r\n                    <small>*indicates required field</small> \r\n                </v-card-text>\r\n                <v-card-actions> \r\n                    <v-spacer></v-spacer> \r\n                    <v-btn color=\"blue darken-1\" text @click=\"dialog = false\">Close</v-btn> \r\n                    <v-btn color=\"blue darken-1\" text @click=\"setForm()\">Save</v-btn> \r\n                </v-card-actions> \r\n            </v-card> \r\n        </v-dialog> \r\n        <v-snackbar \r\n            v-model=\"snackbar\"\r\n            :color=\"color\" \r\n            :multi-line=\"true\" \r\n            :timeout=\"3000\"\r\n        > \r\n            {{ text }} \r\n            <v-btn \r\n                dark \r\n                text \r\n                @click=\"snackbar = false\" \r\n            > \r\n                Close \r\n            </v-btn> \r\n        </v-snackbar> \r\n    </v-container> \r\n</template> \r\n\r\n<script> \r\nexport default { \r\n    data () { \r\n        return { \r\n            dialog: false, \r\n            keyword: '', \r\n            jenis : ['Ringan','Sedang','Berat'],\r\n            headers: [ \r\n                { \r\n                    text: 'No', \r\n                    value: 'no', \r\n                }, \r\n                { \r\n                    text: 'Name', \r\n                    value: 'name' \r\n                }, \r\n                {\r\n                    text: 'Type',\r\n                    value: 'type'\r\n                },\r\n                { \r\n                    text: 'Price', \r\n                    value: 'price' \r\n                }, \r\n                { \r\n                    text: 'Date Created', \r\n                    value: 'created_at' \r\n                    }, \r\n                { \r\n                    text: 'Aksi', \r\n                    value: null \r\n                }, \r\n            ], \r\n            services: [], \r\n            snackbar: false, \r\n            color: null, \r\n            text: '', \r\n            load: false,\r\n            form: { \r\n                name : '', \r\n                type : '', \r\n                price: 0 \r\n            }, \r\n            service : new FormData, \r\n            typeInput: 'new', \r\n            errors : '', \r\n            updatedId : '', \r\n        } \r\n    }, \r\n    methods:{ \r\n        getData(){ \r\n            var uri = this.$apiUrl + '/services' \r\n            this.$http.get(uri).then(response =>{ \r\n                this.services=response.data.message \r\n            }) \r\n        }, \r\n        sendData(){ \r\n                this.service.append('name', this.form.name);\r\n                this.service.append('type', this.form.type);\r\n                this.service.append('price', this.form.price);\r\n                var uri = this.$apiUrl + '/services'\r\n                this.load = true\r\n                this.$http.post(uri, this.service).then(response => {\r\n                    this.snackbar = true; //mengaktifkan snackbar\r\n                    this.color = 'green'; //memberi warna snackbar\r\n                    this.text = response.data.message; //memasukkan pesan ke snackbar\r\n                    this.load = false;\r\n                    this.dialog = false;\r\n                    this.getData(); //mengambil data user\r\n                    this.resetForm();\r\n                }).catch(error => {\r\n                    this.errors = error;\r\n                    this.snackbar = true;\r\n                    this.text = 'Try Again';\r\n                    this.color = 'red';\r\n                    this.load = false;\r\n                })\r\n        }, \r\n        updateData(){ \r\n            this.service.append('name', this.form.name); \r\n            this.service.append('type', this.form.type); \r\n            this.service.append('price', this.form.price); \r\n            var uri = this.$apiUrl + '/services/' + this.updatedId; \r\n            this.load = true \r\n            this.$http.post(uri,this.service).then(response =>{\r\n            this.snackbar = true; //mengaktifkan snackbar \r\n            this.color = 'green'; //memberi warna snackbar \r\n            this.text = response.data.message; //memasukkan pesan ke snackbar \r\n            \r\n            this.load = false; this.dialog = false \r\n            this.getData(); //mengambil data user \r\n            this.resetForm(); this.typeInput = 'new'; \r\n        }).catch(error =>{ \r\n            this.errors = error \r\n            this.snackbar = true;\r\n            this.text = 'Try Again'; \r\n            this.color = 'red'; \r\n            this.load = false; \r\n            this.typeInput = 'new'; \r\n        }) \r\n        }, \r\n        editHandler(item){ \r\n            this.typeInput = 'edit'; \r\n            this.dialog = true; \r\n            this.form.name = item.name; \r\n            this.form.type = item.type; \r\n            this.form.price =item.price, \r\n            this.updatedId = item.id \r\n        }, \r\n        deleteData(deleteId){ //menghapus data \r\n            var uri = this.$apiUrl + '/services/' + deleteId; //data dihapus berdasarkan id \r\n            this.$http.delete(uri).then(response =>{ \r\n                this.snackbar = true; \r\n                this.text = response.data.message; \r\n                this.color = 'green' \r\n                this.deleteDialog = false; \r\n                this.getData(); \r\n            }).catch(error =>{ \r\n                this.errors = error \r\n                this.snackbar = true; \r\n                this.text = 'Try Again'; \r\n                this.color = 'red'; \r\n            }) \r\n        }, \r\n        setForm() {\r\n                if (this.typeInput === 'new') {\r\n                    this.sendData()\r\n                }else {\r\n                    console.log(\"dddd\")\r\n                    this.updateData()\r\n                }\r\n            },\r\n        resetForm(){ \r\n            this.form = { \r\n                name : '', \r\n                type : '', \r\n                price : '' \r\n            } \r\n        } \r\n        }, \r\n        mounted(){ \r\n            this.getData(); \r\n        }, \r\n    } \r\n</script>"],"sourceRoot":"src/components/dashboardContents"}]}